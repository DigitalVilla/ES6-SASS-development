{
  "name": "cooltheme",
  "version": "1.0.0",
  "description": "A wordpress theme",
  "main": "index.js",
  "scripts": {
    "aos:sass": "node-sass sass/sass/aos.scss public/css/aos.css",
    "babel": "babel --presets es2015 js/main.js -o public/main.js",
    "build": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "concat:css": "concat -o public/css/style.concat.css public/css/icon-font.css public/css/style.comp.css",
    "compile:sass": "node-sass sass/main.scss public/css/style.comp.css",
    "compress:css": " node-sass public/css/style.prefix.css public/css/style.min.css --output-style compressed",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" -o public/css/style.prefix.css public/css/style.concat.css",
    "sass": "node-sass sass/main.scss public/css/style.css -w",
    "start": "live-server",
    "dev": "npm-run-all --parallel sass babel start "
  },
  "author": "digital villa",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.4.10",
    "concat": "^1.0.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  },
  "browserslist": {
    "production": [
      "last 10 version"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1"
  }
}
